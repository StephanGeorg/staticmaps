{
  "name": "staticmaps",
  "version": "0.6.1",
  "description": "A node.js library for creating map images with polylines and markers.",
  "main": "./dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --require babel-register",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGeorg/staticmaps.git"
  },
  "keywords": [
    "openstreetmap",
    "osm",
    "staticmaps",
    "staticmap",
    "map",
    "maps"
  ],
  "author": "Stephan Georg <s.georg@nearest.place>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StephanGeorg/staticmaps/issues"
  },
  "homepage": "https://github.com/StephanGeorg/staticmaps#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "gm": "^1.23.0",
    "jimp": "^0.2.27",
    "lodash": "^4.17.4",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ]
  }
}
